<UserControl x:Class="Wff.OpenSource.Views.Demo_Dommel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Wff.OpenSource.Views"
             xmlns:wff="http://wff-toolkits/controls"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <wff:Button Content="Dommel"
                    Click="Button_Click" />
        <wff:ListBox Name="lb"
                     Grid.Row="1"
                     Height="300" />
    </Grid>
</UserControl>
